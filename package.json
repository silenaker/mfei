  "dependencies": {
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/uuid": "^8.0.1",
    "core-js": "^3.6.5",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "start": "npm run clean && npm run dev",
    "dev": "babel -d ./es -D -x .js,.ts -s true -w src",
    "build": "npm run clean && npm run build:es && npm run build:umd && npm run build:types",
    "build:es": "babel -d ./es -D -x .js,.ts -s true src",
    "build:umd": "rollup -c --environment NODE_ENV:production",
    "build:umd:dev": "rollup -c --environment NODE_ENV:development",
    "build:types": "tsc -d --emitDeclarationOnly --outDir types",
    "clean": "rimraf ./es ./umd ./types",
    "test": "karma start ./test/karma.conf.js",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/silenaker/mfi.git"
